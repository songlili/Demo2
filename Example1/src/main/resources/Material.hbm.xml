<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hrm.entity.Material" table="material">
	  <id column="id" name="id" type="integer">
			<generator class="native"/>		<!-- 主键生成策略 -->
		</id>
		<property name="keyword" length="50" type="string"/>
		<property name="date" length="20" type="string"/>
		<property name="file" length="50" type="string"/>
		<property name="description" length="50" type="string"/>
	</class>
</hibernate-mapping>
